Xcursor.theme:	        whiteglass 
 
urxvt*urlLauncher:	firefox3
urxvt*matcher.button:	1
urxvt*perl-ext-common:	matcher
urxvt*loginShell:	true
urxvt*scrollBar:        false
urxvt*background:	#000000
urxvt*foreground:	#eeeeec
urxvt*saveLines:	30000
#urxvt*font:		xft:ProggyTiny:pixelsize=10:antialias=true


urxvt*font: -*-proggytiny-*-*-*-*-10-*-*-*-*-*-*-1
urxvt*boldFont: -*-proggytinysz-bold-*-*-*-10-*-*-*-*-*-*-*
urxvt*italicFont: -*-proggytinysz-*-*-*-*-10-*-*-*-*-*-*-*
urxvt*boldItalicFont: -*-proggytinysz-bold-*-*-*-10-*-*-*-*-*-*-*

urxvt*title:            Mihairu's Terminal
urxvt*internalBorder:   0

urxvt*geometry: 110x74+5+40

